{
  "name": "react-ts-nextjs-electron-boilerplate",
  "version": "0.1.0",
  "private": true,
  "description": "react-ts-nextjs-electron-boilerplate",
  "license": "ISC",
  "author": "Zero0blanK",
  "type": "commonjs",
  "main": "dist/electron/main.js",
  "scripts": {
    "dev": "npm run build:electron &&concurrently \"npm run dev:next\" \"npm run watch:electron\" \"npm run dev:electron\"",
    "dev:next": "next dev",
    "watch:electron": "tsc -p tsconfig.electron.json --watch",
    "dev:electron": "wait-on http://localhost:3000 && electron .",
    "start": "npm run build:electron && electron .",
    "lint": "eslint",
    "type-check": "tsc --noEmit",
    "tsc": "tsc -p tsconfig.electron.json",
    "build": "next build && tsc -p tsconfig.electron.json",
    "build:electron": "tsc -p tsconfig.electron.json"
  },
  "dependencies": {
    "@prisma/client": "^6.17.1",
    "next": "15.5.6",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/electron": "^1.4.38",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.2.1",
    "electron": "^38.3.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.6",
    "prettier": "^3.6.2",
    "prisma": "^6.17.1",
    "tailwindcss": "^4",
    "typescript": "^5",
    "wait-on": "^9.0.1"
  }
}
